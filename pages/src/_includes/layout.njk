---
title: "default"
---
<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        
        <!-- SEO Meta Tags -->
        <title>{{ title | default("École de Rugby Oval Saône - Trévoux") }}</title>
        <meta name="description" content="{{ description | default("École de rugby pour enfants de 4 à 14 ans à Trévoux. Apprentissage du rugby dans un cadre convivial et familial. Inscription ouverte toute l'année.") }}">
        <meta name="author" content="École de Rugby Oval Saône">
        <meta name="robots" content="index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1">
        
        <!-- Geo Tags pour SEO Local -->
        <meta name="geo.region" content="FR-01">
        <meta name="geo.placename" content="Trévoux">
        <meta name="geo.position" content="45.9417;4.7833">
        <meta name="ICBM" content="45.9417, 4.7833">
        
        <!-- Open Graph / Facebook -->
        <meta property="og:type" content="website">
        <meta property="og:url" content="{{ site.url }}{{ page.url }}">
        <meta property="og:title" content="{{ title | default("École de Rugby Oval Saône") }}">
        <meta property="og:description" content="{{ description | default("École de rugby pour enfants de 4 à 14 ans à Trévoux") }}">
        <meta property="og:image" content="{{ site.url }}/assets/logo.png">
        <meta property="og:locale" content="fr_FR">
        
        <!-- Twitter -->
        <meta property="twitter:card" content="summary_large_image">
        <meta property="twitter:url" content="{{ site.url }}{{ page.url }}">
        <meta property="twitter:title" content="{{ title | default("École de Rugby Oval Saône") }}">
        <meta property="twitter:description" content="{{ description | default("École de rugby pour enfants de 4 à 14 ans à Trévoux") }}">
        <meta property="twitter:image" content="{{ site.url }}/assets/logo.png">

        <!-- Canonical URL -->
        <link rel="canonical" href="{{ site.url }}{{ page.url }}">
        
        <!-- Preconnect to external domains for faster loading -->
        <link rel="preconnect" href="https://cdnjs.cloudflare.com" crossorigin>
        <link rel="preconnect" href="https://www.clarity.ms">
        <link rel="preconnect" href="https://www.instagram.com">
        <link rel="dns-prefetch" href="https://ligueaura.ffr.fr">
        <link rel="dns-prefetch" href="https://www.auvergnerhonealpes.fr">
        <link rel="dns-prefetch" href="https://cd01rugby.com">
        
        <!-- Preload critical resources -->
        {% if page.url == "/" %}
        <link rel="preload" href="/assets/logo-48x48.webp" as="image" type="image/webp">
        <link rel="preload" href="/assets/logo-240x230.webp" as="image" type="image/webp">
        <link rel="preload" href="/assets/hero-background.webp" as="image" type="image/webp">
        {% endif %}
        <link rel="preload" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" as="style" onload="this.onload=null;this.rel='stylesheet'">
        <noscript><link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"></noscript>
        
        <!-- Favicon -->
        <link rel="icon" type="image/x-icon" href="/assets/favicon.ico">
        <link rel="apple-touch-icon" sizes="180x180" href="/assets/apple-touch-icon.png">
        <link rel="icon" type="image/png" sizes="32x32" href="/assets/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="/assets/favicon-16x16.png">
        <link rel="manifest" href="/site.webmanifest">
        <meta name="theme-color" content="#ffffff">
        <meta name="apple-mobile-web-app-capable" content="yes">
        <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
        <meta name="mobile-web-app-capable" content="yes">
        
        <!-- Stylesheets -->
        <link rel="stylesheet" href="/css-bundle.css">
        
        <script type="text/javascript">
            (function(c,l,a,r,i,t,y){
                c[a]=c[a]||function(){(c[a].q=c[a].q||[]).push(arguments)};
                t=l.createElement(r);t.async=1;t.src="https://www.clarity.ms/tag/"+i;
                y=l.getElementsByTagName(r)[0];y.parentNode.insertBefore(t,y);
            })(window, document, "clarity", "script", "sk1wnmwiec");
        </script>

        <!-- Schema.org JSON-LD -->
        <script type="application/ld+json">
        {
            "@context": "https://schema.org",
            "@type": "SportsClub",
            "name": "École de Rugby Oval Saône",
            "description": "École de rugby pour enfants de 4 à 14 ans dans un cadre convivial et familial",
            "url": "{{ site.url }}",
            "logo": "{{ site.url }}/assets/logo.png",
            "sport": "Rugby",
            "address": {
                "@type": "PostalAddress",
                "streetAddress": "Chemin de la passerelle",
                "addressLocality": "Quincieux",
                "postalCode": "69650",
                "addressCountry": "FR"
            },
            "telephone": "06 83 62 27 10",
            "email": "edr.ovalsaone@gmail.com",
            "geo": {
                "@type": "GeoCoordinates",
                "latitude": "45.9417",
                "longitude": "4.7833"
            },
            "areaServed": [
                {
                    "@type": "City",
                    "name": "Trévoux"
                },
                {
                    "@type": "City",
                    "name": "Sainte-Euphémie"
                },
                {
                    "@type": "City",
                    "name": "Quincieux"
                },
                {
                    "@type": "City",
                    "name": "Saint-Germain-au-Mont-d'Or"
                },
                {
                    "@type": "City",
                    "name": "Lyon"
                },
                {
                    "@type": "City",
                    "name": "Saint-Didier-de-Formans"
                },
                {
                    "@type": "City",
                    "name": "Toussieux"
                },
                {
                    "@type": "State",
                    "name": "Ain"
                }
            ],
            "memberOf": {
                "@type": "SportsOrganization",
                "name": "Fédération Française de Rugby",
                "url": "https://www.ffr.fr"
            },
            "parentOrganization": {
                "@type": "SportsOrganization",
                "name": "Ligue Auvergne-Rhône-Alpes Rugby",
                "url": "https://ligueaura.ffr.fr"
            },
            "sameAs": [
                "https://www.instagram.com/ovalsaone/",
                "https://www.facebook.com/profile.php?id=61580067910711"
            ]
        }
        </script>

        <!-- FAQ Schema.org désormais dans faq.liquid (dynamique depuis page_faq.json) -->

        <!-- Breadcrumb Schema.org -->
        {% if page.url != "/" %}
        <script type="application/ld+json">
        {
            "@context": "https://schema.org",
            "@type": "BreadcrumbList",
            "itemListElement": [
                {
                    "@type": "ListItem",
                    "position": 1,
                    "name": "Accueil",
                    "item": "{{ site.url }}/"
                }
                {% if page.url == "/equipes/" %},
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "Nos Équipes"
                }
                {% elif page.url == "/ecole/" %},
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "L'École"
                }
                {% elif page.url == "/evenements/" %},
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "Calendrier"
                }
                {% elif page.url == "/inscription/" %},
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "Inscription"
                }
                {% elif page.url == "/contact/" %},
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "Contact"
                }
                {% elif page.url == "/partenariat/" %},
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "Partenariat & Dons"
                }
                {% elif page.url == "/faq/" %},
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "FAQ"
                }
                {% elif page.url == "/boutique/" %},
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "Boutique"
                }
                {% elif page.url == "/faq/" %},
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "FAQ"
                }
                {% endif %}
            ]
        }
        </script>
        {% endif %}
    </head>
    <body>
        <!-- Bandeau cookies RGPD -->
        <div id="cookie-banner" class="cookie-banner hidden">
            <div class="cookie-content">
                <p>Ce site utilise des cookies pour améliorer votre expérience. En continuant à naviguer, vous acceptez notre
                    utilisation des cookies.</p>
                <div class="cookie-buttons">
                    <button id="accept-cookies" class="btn btn-primary">Accepter</button>
                    <button id="decline-cookies" class="btn btn-secondary">Refuser</button>
                </div>
            </div>
        </div>
        <!-- Navigation fixe -->
        <div class="menu-overlay" id="menu-overlay"></div>
        <nav class="navbar" id="navbar">
            <div class="nav-container">
                <a href="/" class="nav-logo">
                    <img src="/assets/logo-96x96.webp" alt="Logo Association Rugby" id="nav-logo-img" loading="eager">
                    <span>Oval Saône</span>
                </a>
                <div class="nav-menu" id="nav-menu">
                    <a href="/" class="nav-link {% if page.url == "/" %} active {% endif %}">Accueil</a>
                    <a href="/equipes" class="nav-link {% if page.url == "/equipes/" %} active {% endif %}">Équipes</a>
                    <a href="/ecole" class="nav-link {% if page.url == "/ecole/" %} active {% endif %}">L'École</a>
                    <a href="/evenements" class="nav-link {% if page.url == "/evenements/" %} active {% endif %}">Calendrier</a>
                    <a href="/partenariat" class="nav-link {% if page.url == "/partenariat/" %} active {% endif %}">Partenariat & Dons</a>
                    <a href="/boutique" class="nav-link {% if page.url == "/boutique/" %} active {% endif %}">Boutique</a>
                    <a href="/inscription" class="nav-link {% if page.url == "/inscription/" %} active {% endif %}">Inscription</a>
                </div>
                <div class="nav-toggle" id="nav-toggle">
                    <span class="bar"></span>
                    <span class="bar"></span>
                    <span class="bar"></span>
                </div>
            </div>
        </nav>
        {{ content | safe }}

        <nav class="mobile-bottom-nav" aria-label="Navigation principale mobile">
            <a href="/evenements" class="mobile-bottom-nav__link{% if page.url == "/evenements/" %} is-active{% endif %}" aria-label="Calendrier"{% if page.url == "/evenements/" %} aria-current="page"{% endif %}>
                <i class="fas fa-calendar-alt" aria-hidden="true"></i>
                <span>Calendrier</span>
            </a>
            <a href="/" class="mobile-bottom-nav__link mobile-bottom-nav__link--center{% if page.url == "/" %} is-active{% endif %}" aria-label="Accueil"{% if page.url == "/" %} aria-current="page"{% endif %}>
                <i class="fas fa-home" aria-hidden="true"></i>
                <span>Accueil</span>
            </a>
            <a href="/inscription" class="mobile-bottom-nav__link{% if page.url == "/inscription/" %} is-active{% endif %}" aria-label="Inscription"{% if page.url == "/inscription/" %} aria-current="page"{% endif %}>
                <i class="fas fa-user-plus" aria-hidden="true"></i>
                <span>Inscription</span>
            </a>
        </nav>
        <!-- Footer -->
        <footer class="footer">
            <div class="container">
                <div class="footer-grid">
                    <div class="footer-section">
                        <h3>Contact</h3>
                        <p>
                            <i class="fas fa-envelope"></i>
                            {{ contact.email }}</p>
                        <p>
                            <i class="fas fa-phone"></i>
                            {{ contact.phone }}</p>
                        <p>
                            <i class="fas fa-map-marker-alt"></i>
                            {{ contact.address }}</p>
                    </div>
                    <div class="footer-section">
                        <h3>Liens rapides</h3>
                        <ul>
                            <li><a href="/inscription">Inscription</a></li>
                            <li><a href="/equipes">Nos équipes</a></li>
                            <li><a href="/rugby-enfants-trevoux">Rugby pour Enfants</a></li>
                            <li><a href="/contact">Contact</a></li>
                            <li><a href="/faq">FAQ</a></li>
                        </ul>
                    </div>
                    <div class="footer-section">
                        <h3>Suivez-nous</h3>
                        <div class="social-links">
                            <a href="https://www.facebook.com/profile.php?id=61580067910711" target="_blank" rel="noopener noreferrer" aria-label="Facebook" class="social-link">
                                <i class="fab fa-facebook-f"></i>
                            </a>
                            <a href="https://www.instagram.com/ovalsaone" target="_blank" rel="noopener noreferrer" aria-label="Instagram" class="social-link">
                                <i class="fab fa-instagram"></i>
                            </a>
                        </div>
                        <div class="instagram-embed">
                            <iframe
                                src="https://www.instagram.com/ovalsaone/embed"
                                width="400"
                                height="480"
                                title="Instagram Feed"
                                loading="lazy"
                                style="border: none; overflow: hidden; width: 100%; max-width: 300px; height: 350px;"></iframe>
                        </div>
                    </div>
                </div>
                <div class="footer-sponsors">
                    <div class="footer-sponsor-logos">
                        <a href="https://ligueaura.ffr.fr/" target="_blank" rel="noopener noreferrer" title="Visiter le site de la Ligue Auvergne-Rhône-Alpes de Rugby">
                            <img src="/assets/aura-rugby.png" alt="Logo Rugby Auvergne Rhône-Alpes" class="footer-logo" loading="lazy">
                        </a>
                        <a href="https://www.auvergnerhonealpes.fr/" target="_blank" rel="noopener noreferrer" title="Visiter le site de la Région Auvergne-Rhône-Alpes">
                            <img src="/assets/aura-region.png" alt="Logo Région Auvergne Rhône-Alpes" class="footer-logo" loading="lazy">
                        </a>
                        <a href="https://cd01rugby.com/" target="_blank" rel="noopener noreferrer" title="Visiter le site du Comité de Rugby de l'Ain">
                            <img src="/assets/ain-rugby.png" alt="Logo Comité de Rugby de l'Ain" class="footer-logo" loading="lazy">
                        </a>
                    </div>
                </div>
                <div class="footer-bottom">
                    <p>&copy; 2025 Rugby Club. Tous droits réservés.</p>
                </div>
            </div>
        </footer>

        <!-- Cloudflare Turnstile (mode explicite) -->
        <script src="https://challenges.cloudflare.com/turnstile/v0/api.js?render=explicit" async defer></script>
        <script>window.TURNSTILE_SITE_KEY = "{{ turnstile.siteKey }}";</script>

        <script src="/bundle.js"></script>
    </body>
</html>
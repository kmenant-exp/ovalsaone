{
  "name": "vanilla-basic",
  "version": "1.0.0",
  "scripts": {
    "start": "sirv ./src public --cors --single --no-clear --port 8000",
    "dev": "swa start src --api-location src/api --host 127.0.0.1 --port 4280",
    "build:api": "cd /src/api && dotnet build",
    "clean:api": "cd /src/api && dotnet clean"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.6"
  }
}
